{
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "6.10"
          }
        }],
        "@babel/preset-react"
      ],
      "plugins": [
        // "transform-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
      ]
    },
    "browser": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": "last 2 versions, ie 10-11"
          },
          "modules": false
        }],
        "@babel/preset-react"
      ],
      "plugins": [
        // "transform-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
      ]
    },
    "module": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "6.10"
          },
          "modules": false
        }],
        "@babel/preset-react"
      ],
      "plugins": [
        // "transform-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
      ]
    }
  },
  "sourceMaps": true
}