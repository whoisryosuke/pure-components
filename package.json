{
  "name": "pure-styled-components",
  "version": "1.1.0",
  "description": "Styled Component design system based on Pure CSS for React/Preact",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -R dist && babel src -d dist --ignore '**.story.js','**.test.js'",
    "build-commonjs": "BABEL_ENV=commonjs babel src --out-dir commonjs --ignore '*.story.js','*.test.js' --source-maps inline",
    "build-esm": "BABEL_ENV=esm babel src --out-dir esm --ignore '*.story.js','*.test.js' --source-maps inline",
    "dev": "npm run storybook",
    "docs": "npx styleguidist server",
    "docs:build": "npx styleguidist build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whoisryosuke/pure-components.git"
  },
  "keywords": [
    "design-system",
    "design-systems",
    "component-system",
    "react-components",
    "components",
    "pure-css",
    "css-in-js"
  ],
  "author": "Ryosuke",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "jest": "^23.6.0",
    "prettier": "1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.0",
    "webpack": "^4.28.3"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "prop-types": "^15.6.2",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.0"
  },
  "dependencies": {},
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>tests/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/tests/setup/"
    ]
  }
}